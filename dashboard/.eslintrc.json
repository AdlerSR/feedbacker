{
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"parserOptions": {
		"parser": "babel-eslint"
	},
	"extends": [
		"airbnb-base",
		"plugin:vue/recommended",
		"prettier"
	],
	"rules": {
		"prettier/prettier": "error",
		"no-unused-vars": "warn",
		"vue/no-multiple-template-root": "off"
	}
}